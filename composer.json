{
    "name": "laravel-notification-channels/bearychat",
    "description": "BearyChat notifications channel for Laravel.",
    "homepage": "https://github.com/laravel-notification-channels/bearychat",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://github.com/ElfSundae",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "elfsundae/laravel-bearychat": "^1.2",
        "illuminate/notifications": "^5.3",
        "illuminate/queue": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\BearyChat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\BearyChat\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
